{
  "version": "1.1.0-beta.4",
  "id": "@shopgate-project/page-switcher",
  "components": [
    {
      "id": "SwitchHeader",
      "type": "portals",
      "path": "frontend/components/SwitchHeader",
      "target": "app-bar.right"
    },
    {
      "id": "Logo",
      "path": "frontend/components/Logo",
      "target": "app-bar.center",
      "type": "portals"
    },
    {
      "id": "HideBackArrow",
      "path": "frontend/components/HideBackArrow",
      "target": "app-bar.left",
      "type": "portals"
    },
    {
      "id": "SwitchHomeTabBarAction",
      "path": "frontend/components/SwitchHomeTabBarAction",
      "target": "tab-bar.home",
      "type": "portals"
    },
    {
      "id": "SwitchSelection",
      "path": "frontend/reducers",
      "type": "reducers"
    },
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions",
      "type": "subscribers"
    }
  ],
  "configuration": {
    "pageLinking": {
      "type": "admin",
      "destination": "frontend",
      "default": [],
      "params": {
        "type": "json",
        "label": "Page linking"
      }
    },
    "linkColor": {
      "type": "admin",
      "destination": "frontend",
      "default": "",
      "params": {
        "type": "text",
        "label": "Color of the links in the header"
      }
    },
    "linkSelectedColor": {
      "type": "admin",
      "destination": "frontend",
      "default": "",
      "params": {
        "type": "text",
        "label": "Color of the selected link"
      }
    }
  }
}
